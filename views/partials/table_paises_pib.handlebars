<table class="table table-bordered table-sm">
  <thead>
    <th>Nombre</th>
    <th>Pib 2019</th>
    <th>Pib 2020</th>
    <th>Diferencia</th>    
  </thead>
  <tbody>
    {{#each paises}} 
    <tr>
      <td>{{this.nombre}}</td>
      <td>{{this.pib_2019}}</td>
      <td>{{this.pib_2020}}</td>                  
      <td>{{this.diference}}</td>                  
    </tr>
    {{/each}}
  </tbody>
</table>

